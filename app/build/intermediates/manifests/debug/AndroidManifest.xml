<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.innovation_day"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />
    compileSdkVersion 'android-L'

    <!-- Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@drawable/infa"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.innovation_day.AndroidTabLayoutActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- DataManager Activity -->
        <activity android:name="com.example.innovation_day.GetPkgDisplay" />
        <activity android:name="com.example.innovation_day.Get_Rowidobject_List" />
        <activity android:name="com.example.innovation_day.ActivityUnmerge" />
        <activity android:name="com.example.innovation_day.UnmergeActivity" />
        <activity android:name="com.example.innovation_day.MergeRecordsActivity" />
        <activity
            android:name="com.example.innovation_day.DialogActivity"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.example.innovation_day.AddConsumer"
            android:label="Add New Consumer" >
        </activity>

        <!-- Videos Activity -->
        <activity android:name="com.example.innovation_day.VideosActivity" />

        <!-- Photos Activity -->
        <activity android:name="com.example.innovation_day.PhotosActivity" />
    </application>

</manifest>